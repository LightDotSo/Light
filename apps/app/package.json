{
  "name": "@lightdotso/app",
  "version": "1.1.0",
  "private": false,
  "homepage": "https://github.com/LightDotSo/LightDotSo",
  "bugs": {
    "url": "https://github.com/LightDotSo/LightDotSo/issues"
  },
  "repository": "https://github.com/LightDotSo/LightDotSo",
  "license": "GPL-3.0",
  "author": "Light, Inc.",
  "scripts": {
    "build": "pnpm run prisma:cp && pnpm run prisma:generate && next build",
    "coverage": "nyc report && nyc check-coverage --lines 95",
    "cypress:ci": "cypress run --record",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev",
    "e2e": "start-server-and-test dev:instrumented http://localhost:3000 cypress:run",
    "e2e:open": "start-server-and-test dev:instrumented http://localhost:3000 cypress:open",
    "dev:instrumented": "cross-env CYPRESS_INSTRUMENT_CODE=1 next dev",
    "doppler:setup": "doppler setup --no-interactive",
    "env:local": "doppler secrets download --no-file --format env > .env.local",
    "env:prisma": "doppler secrets download --no-file --format env > prisma/.env",
    "env:vercel": "vercel env pull .env.local",
    "fix": "cd ../.. && pnpm run fix",
    "prisma:cp": "cp ../../packages/prisma/schema.prisma prisma/schema.prisma",
    "prisma:db:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "start": "next start",
    "test": "jest"
  },
  "dependencies": {
    "@cyberlab/cyberconnect": "^4.5.0",
    "@cyberlab/social-verifier": "^2.0.1",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@lightdotso/const": "workspace:*",
    "@lightdotso/core": "workspace:*",
    "@lightdotso/middlewares": "workspace:*",
    "@lightdotso/services": "workspace:*",
    "@lightdotso/styles": "workspace:*",
    "@lightdotso/tailwindcss": "workspace:*",
    "@lightdotso/tsconfig": "workspace:*",
    "@lightdotso/types": "workspace:*",
    "@popperjs/core": "^2.11.5",
    "@prisma/client": "^4.1.1",
    "@sentry/integrations": "^7.8.1",
    "@sentry/nextjs": "^7.8.1",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.1",
    "critters": "^0.0.16",
    "ethers": "^5.6.9",
    "framer-motion": "^6.5.1",
    "javascript-time-ago": "^2.5.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "^12.2.3",
    "next-auth": "^4.10.3",
    "next-compose-plugins": "^2.2.1",
    "next-plausible": "^3.2.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.0",
    "next-transpile-modules": "^9.0.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.3.0",
    "react-icons": "^4.4.0",
    "react-time-ago": "^7.2.1",
    "recoil": "^0.7.4",
    "siwe": "^1.1.6",
    "swr": "^1.3.0",
    "vanilla-tilt": "^1.7.2",
    "wagmi": "^0.2.28",
    "zod": "^3.17.10",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react": "^6.0.0",
    "@jest/globals": "^28.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.15",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.3.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "nyc": "^15.1.0",
    "postcss": "^8.4.14",
    "prisma": "^4.1.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "prisma": {
    "schema": "../../packages/prisma/schema.prisma"
  }
}
